   <Row>
    <Cell ss:Index="2"><Data ss:Type="String">am or pm?</Data></Cell>
    <Cell ss:StyleID="s25"><Data ss:Type="String">{R;cond=WHEN value-TIME GE '120000' THEN 'post meridiem' ELSE 'ante meridiem'}</Data></Cell>
   </Row>
   <Row>
    <Cell ss:Index="2"><Data ss:Type="String">Number of lines in R-T table</Data><NamedCell
      ss:Name="Print_Titles"/></Cell>
    <Cell ss:StyleID="s23"><Data ss:Type="String">{R;cond=`Time= ` &amp;&amp; sy-uzeit(2) &amp;&amp; ':' &amp;&amp; sy-uzeit+2(2) &amp;&amp; ':' &amp;&amp; sy-uzeit+4(2)}</Data><Comment
      ss:Author="Moldabayev, Birzhan"><ss:Data
       xmlns="http://www.w3.org/TR/REC-html40"><B><Font html:Face="Tahoma"
         x:CharSet="1" html:Size="9" html:Color="#000000">modekz:</Font></B><Font
        html:Face="Tahoma" x:CharSet="1" html:Size="9" html:Color="#000000">&#10;in new ABAP version could be String Templates&#10;|Number of lines { LINES( value-t ) } at { sy-datum DATE = USER }|</Font></ss:Data></Comment><NamedCell
      ss:Name="Print_Titles"/></Cell>
   </Row>
   <Row>
    <Cell ss:Index="2"><Data ss:Type="String">Read table value by []</Data><NamedCell
      ss:Name="Print_Titles"/></Cell>
    <Cell ss:StyleID="s23"><Data ss:Type="String">{R;cond=WHEN line_exists( value-t[ group = 'GRP A' ] ) THEN |First caption in group 'A' { value-t[ group = 'GRP A' ]-caption }|}</Data><NamedCell
      ss:Name="Print_Titles"/></Cell>
   </Row>
   <Row ss:Height="15.75">
    <Cell ss:Index="2"><Data ss:Type="String">Additional payment</Data><NamedCell
      ss:Name="Print_Titles"/></Cell>
    <Cell ss:StyleID="s23"><Data ss:Type="String">{R;cond=WHEN value-DATE(4) = '2020' THEN 42500 ELSE 0;type=double}</Data><NamedCell
      ss:Name="Print_Titles"/></Cell>
   </Row>
   <Row ss:Height="45.75">
    <Cell ss:StyleID="s18"><Data ss:Type="String">â„–</Data><NamedCell
      ss:Name="Print_Titles"/></Cell>
