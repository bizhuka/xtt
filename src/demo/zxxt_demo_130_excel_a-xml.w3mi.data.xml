    <Cell ss:StyleID="s18"><Data ss:Type="String">SUM 2</Data><NamedCell
      ss:Name="Print_Titles"/></Cell>
    <Cell ss:Index="7" ss:StyleID="s24"><Data ss:Type="String">Which field is bigger?&#10;Result is STRING by default</Data><NamedCell
      ss:Name="Print_Titles"/></Cell>
    <Cell ss:StyleID="s24"><Data ss:Type="String">Plus (GROUP cp '*A*'  ) ELSE minus&#10;Covert result to double</Data><NamedCell
      ss:Name="Print_Titles"/></Cell>
   </Row>
   <Row ss:Height="45">
    <Cell ss:StyleID="s19"><Data ss:Type="String">{R-T;cond=sy-tabix;type=integer}</Data></Cell>
    <Cell ss:StyleID="s19"><Data ss:Type="String">{R-T-CAPTION}</Data></Cell>
    <Cell ss:StyleID="s20"><Data ss:Type="String">{R-T-GROUP}</Data></Cell>
    <Cell ss:StyleID="s21"><Data ss:Type="String">{R-T-SUM1}</Data></Cell>
    <Cell ss:StyleID="s21"><Data ss:Type="String">{R-T-SUM2}</Data></Cell>
    <Cell ss:Index="7" ss:StyleID="s23"><Data ss:Type="String">{R-T;cond=WHEN value-SUM1 GT value-sum2 THEN 'Sum 1 is bigger' ELSE 'Sum2 is bigger'}</Data></Cell>
